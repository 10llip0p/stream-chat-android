        <item name="streamUiUnmuteOptionIcon">@drawable/stream_ui_ic_umnute</item>
        <item name="streamUiMuteUserEnabled">true</item>
        <item name="streamUiBlockOptionIcon">@drawable/stream_ui_ic_user_block</item>
        <item name="streamUiBlockUserEnabled">true</item>
        <item name="streamUiDeleteOptionIcon">@drawable/stream_ui_ic_delete</item>
